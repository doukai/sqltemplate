update(table,sets,conditionals) ::= <<
UPDATE <table> t
SET (<sets:{set|<set.column> = <set.value>};separator=", ">)
<if(conditionals)>WHERE <conditionals:{conditional|<conditional>};separator=" AND "><endif>
>>
updateAll(records) ::= <<
<records:{record|<update(record.tableName,record.valueSets,records.conditionals)>};separator=";">
>>